pdf_html <- function(pdf){
infos <- pdf_info(pdf) #pegas as info do pdf
html <- tempfile(flieext = '.html') #cria um arquivo temporário
comando <- sprintf('pdftohtml -f 1 -l %s -q -i -s -noframes %s %s',
infos$pages,
normalizePath(pdf),
html)
system(command)
txt <- readr::read_file(html)
file.remove(html)
txt
}
